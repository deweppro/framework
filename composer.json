{
  "name": "deweppro/framework",
  "description": "RESTFul API framework",
  "keywords": [
    "framework",
    "deweppro"
  ],
  "homepage": "https://framework.dewep.pro",
  "support": {
    "issues": "https://github.com/deweppro/framework/issues",
    "source": "https://github.com/deweppro/framework"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Mikhail Knyazhev",
      "email": "markus621@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Dewep\\": "src/Dewep/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dewep\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "ext-mbstring": "*",
    "deweppro/client": "^1.2",
    "deweppro/db": "^1.0",
    "deweppro/http": "^1.0",
    "deweppro/patterns": "^1.0",
    "monolog/monolog": "^1.0",
    "nesbot/carbon": "^2.10",
    "psr/log": "^1.1",
    "symfony/console": "^4.2",
    "symfony/yaml": "^3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "7.2.0"
    }
  },
  "optimize-autoloader": true,
  "scripts": {
    "post-autoload-dump": [
      "php vendor/bin/phpunit"
    ]
  }
}
